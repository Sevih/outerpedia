{
  "name": "outerpedia-clean",
  "version": "10.0.12",
  "private": true,
  "scripts": {
    "//1": "=== CORE ===",
    "dev": "node scripts/dev-with-webp.js",
    "prebuild": "node scripts/prebuild.js",
    "build": "rm -rf .next && next build",
    "start": "next start",
    "lint": "next lint",
    "//2": "=== VALIDATION ===",
    "validate:build": "ls .next/static/css/*.css && ls .next/static/chunks/app/page-*.js && echo âœ… Build is valid",
    "validate:upgrade": "npx next info && npm run build && npm run start -- -p 0",
    "validate:effects": "node scripts/validate-effects.js",
    "//3": "=== DATA GENERATION (run via prebuild) ===",
    "set-version": "node scripts/set-version.js",
    "gen:events": "node scripts/generateEventRegistry.js",
    "inject-sw-version": "node scripts/inject-sw-version.js",
    "generate:allchar": "tsx scripts/generateAllCharacters.ts",
    "generate:slugToChar": "tsx scripts/generateLinkSlugChar.ts",
    "generate:gear-stats": "node scripts/generateGearUsageStats.js",
    "generate:guide-chars": "node scripts/extract-guide-characters.js",
    "gen:pool": "node scripts/generate-pool-data.cjs",
    "generate:gear-submap": "node scripts/generateSubstatsMap.js",
    "build:effects": "node scripts/buildEffectIndex.mjs",
    "gen:effect-types": "node scripts/generate-effect-types.js",
    "gen:news-cache": "node scripts/generateNewsCache.js",
    "//4": "=== WALLPAPERS (from datamine/extracted_astudio) ===",
    "gen:wallpapers": "node scripts/wallpapers.js json",
    "gen:wallpapers:full": "node scripts/wallpapers.js full",
    "gen:wallpapers:scan": "node scripts/wallpapers.js scan",
    "gen:wallpapers:check": "node scripts/wallpapers.js check",
    "//wallpapers": "json=regen JSON | full=extract+dedupe+copy | scan=dry run | check=find dupes in public/",
    "//5": "=== SEO ===",
    "seo-check": "node scripts/seo-check.cjs",
    "seo:audit": "node scripts/seo-audit.js",
    "seo:check": "npm run seo:audit",
    "//6": "=== GIT ===",
    "commit:dry": "node scripts/commit.mjs --dry-run",
    "commit": "node scripts/commit.mjs --no-changelog",
    "commit:fast": "node scripts/commit.mjs --no-changelog --skip-build",
    "//7": "=== NEWS ===",
    "migrate:news": "node scripts/migrate-news-to-markdown.mjs",
    "check:news": "node scripts/scrape-all-va-notices.mjs && node scripts/optimize-news-titles",
    "//8": "=== BOSS / RAID ===",
    "boss:add-buff-debuff": "node scripts/add-buff-debuff.js",
    "boss:copy-buff-debuff": "python scripts/copy-buff-debuff.py",
    "raid:validate": "node scripts/raid/validate.js",
    "//9": "=== GUIDES ===",
    "guide:check": "python scripts/check-guide-format.py"
  },
  "dependencies": {
    "@radix-ui/react-hover-card": "^1.1.11",
    "@radix-ui/react-popover": "^1.1.11",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-tooltip": "^1.2.3",
    "@tailwindcss/typography": "^0.5.19",
    "cheerio": "^1.0.0",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.0",
    "framer-motion": "^12.18.1",
    "gray-matter": "^4.0.3",
    "jsdom": "^26.1.0",
    "jsonc-parser": "^3.3.1",
    "keen-slider": "^6.8.6",
    "lucide-react": "^0.513.0",
    "lz-string": "^1.5.0",
    "markdown-it": "^14.1.0",
    "marked": "^15.0.12",
    "mysql2": "3.15.3",
    "nanoid": "5.1.6",
    "next": "15.5.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "recharts": "^2.15.3",
    "sharp": "^0.34.1",
    "xml2js": "^0.6.2",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^20.19.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "fast-glob": "^3.3.3",
    "node-html-parser": "7.0.1",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "tree-kill": "^1.2.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "turndown": "^7.2.2",
    "typescript": "^5.9.2"
  }
}
